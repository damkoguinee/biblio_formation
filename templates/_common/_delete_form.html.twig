<form method="post" action="{{ path('app_admin_'~ entite ~ '_delete', {'id': entite.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entite.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>